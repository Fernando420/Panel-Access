<div class="row">
    <table class="table table-striped">
        <thead>
            <th>Email</th>
            <th>Phone</th>
            <th>Username</th>
            <th>Role</th>
            <th>Edit</th>
            <th>Show</th>
        </thead>
        <tbody>
            <% @users.each do |user| %>
                <tr>
                    <td><%= user['email'] %></td>
                    <td><%= user['phone'] %></td>
                    <td><%= user['username'] %></td>
                    <td><%= user['role']['name'] %></td>
                    <td><a href="/users/<%= user['id'] %>/edit">Edit</a></td>
                    <td><a href="/users/<%= user['id'] %>">Show</a></td>
                </tr>
            <%end%>
        </tbody>
    </table>
</div>