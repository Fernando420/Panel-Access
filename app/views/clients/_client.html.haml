%tr
  %td= image_tag "#{client['picture'].present? ? client['picture'] : 'clients/missing.png'}", class: 'img-circle', id: ''
  %td= client['name']
  %td= client['last_name']
  %td= client['member_id']
  %td= client['email']
  %td
    %a{href: "/clients/#{client['id']}/edit"}=t('buttons.edit')
  %td
    %a{href: "/clients/#{client['id']}"}=t('buttons.show')
    